{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "bursts",
  "version": "0.1.0",
  "identifier": "fun.xwink",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "【气质花】按键助手",
        "width": 320,
        "height": 480,
        "resizable": false,
        "maximizable": false,
        "center": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
